{
  "Comment": "Extract video metadata using Rekognition.",
  "StartAt": "ExtractMetadataState",
  "States": {
    "ExtractMetadataState": {
      "Type": "Task",
      "Resource": "${RekognitionFunction.Arn}",
      "Next": "CreateEventState"
    },
    "CreateEventState": {
      "Type" : "Task",
      "Resource": "${VideoMetadataEventFunction.Arn}",
      "End": true
    }
  }
}
